  "bugs": {
    "url": "https://github.com/danielcpan/ChatApp/issues"
  },
  "homepage": "https://github.com/danielcpan/ChatApp#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-jwt": "^5.3.1",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.19.1",
    "pg": "^7.12.0",
